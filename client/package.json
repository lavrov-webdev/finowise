{
  "name": "rsbuild-react-ts",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build",
    "build:analyze": "BUNDLE_ANALYZE=true rsbuild build",
